{
  "problemMatcher": [
    {
      "owner": "assert-problem-matcher",
      "pattern": [
        {
          "regexp": "^.*\\((.*):(\\d+).*\\) (Assertion failed|Unhandled Exception.*)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    },
    {
      "owner": "warning-problem-matcher",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\[.+\\] {C..:\\w+} W \\((\\S+):(\\d+) \\S+\\) (.+)",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    },
    {
      "owner": "error-problem-matcher",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\[.+\\] {C..:\\w+} E \\((\\S+):(\\d+) \\S+\\) (.+)",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}
